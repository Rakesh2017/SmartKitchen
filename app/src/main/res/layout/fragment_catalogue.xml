<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroundwithoutsklogo"
    tools:context="com.example.dell.smartkitchen.CatalogueFrag">

    <!-- TODO: Update blank fragment layout -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >

    <TextView
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:text="Catalogue List"
        android:textSize="30dp"
        android:gravity="center"
        android:background="#fd3232"
        android:textStyle="bold"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="40dp"
        android:layout_marginRight="20dp"
        android:gravity="center"
        android:text="Number of boxes deployed  : "
        android:textSize="20dp"
        android:layout_marginLeft="20dp"
        android:textStyle="bold"
        android:background="#b4842b"
        android:textColor="#000000"
        android:id="@+id/cl1"
        />



    </LinearLayout>


</FrameLayout>
